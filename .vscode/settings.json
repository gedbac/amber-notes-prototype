{
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "afterDelay",
  "files.eol": "\r\n",
  "files.exclude": {
    "**/node_modules": true,
  },

  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.rulers": [ 120 ],
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
}